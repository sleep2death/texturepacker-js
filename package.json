{
  "name": "texturepacker-js",
  "version": "1.0.0",
  "description": "a texturepacker to pack all textures into one spritesheet",
  "main": "index.js",
  "scripts": {
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleep2death/texturepacker-js.git"
  },
  "keywords": [
    "texturepacker",
    "spritesheet",
    "texture",
    "packer"
  ],
  "xo": {
    "esnext": true,
    "env": [
      "node",
      "browser"
    ],
    "space": true,
    "semicolon": false,
    "rules": {
      "curly": 0,
      "keyword-spacing": 0,
      "guard-for-in": 0
    },
    "globals": [
      "Phaser",
      "$"
    ]
  },
  "author": "aspirin2d",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sleep2death/texturepacker-js/issues"
  },
  "homepage": "https://github.com/sleep2death/texturepacker-js#readme",
  "dependencies": {
    "async": "^2.0.0",
    "binpacking": "0.0.1",
    "chalk": "^1.1.3",
    "inquirer": "^1.1.2",
    "js-yaml": "^3.6.1",
    "mkdirp": "^0.5.1",
    "optimist": "^0.6.1",
    "platform-command": "0.0.1",
    "plist": "^1.2.0",
    "progress": "^1.1.8",
    "xlsx": "^0.8.0"
  }
}
